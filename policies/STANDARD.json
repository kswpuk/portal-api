{
  "Statement": [
    {
      "Sid": "rest",
      "Action": "execute-api:Invoke",
      "Effect": "Allow",
      "Resource": [
        "arn:aws:execute-api:*:*:*/*/GET/events",
        "arn:aws:execute-api:*:*:*/*/GET/members",
        "arn:aws:execute-api:*:*:*/*/GET/members/{membershipNumber}",
        "arn:aws:execute-api:*:*:*/*/PUT/members/{membershipNumber}",
        "arn:aws:execute-api:*:*:*/*/GET/members/*/photo"
      ]
    }
  ]
}